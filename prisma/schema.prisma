generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Producto {
  id           Int     @id @default(autoincrement())
  titulo       String
  precio       String
  descripcion  String
  imagen       String
}
